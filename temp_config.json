{
  "assay_data": {
    "alias": {
       "description": "unique name in the experiment (Assay Name in SDRF)",
       "type": "string",
       "import": {
         "ae": {
           "from_type": "string",
           "parent": "assay_data",
           "path": ["alias"],
           "method": "import_string"
         },
         "hca": {
          "path": [],
          "from_type": "string",
          "method": "get_hca_bundle_uuid"
        }
       }
     },
    "files": {
      "description": "list of DataFile class objects",
      "type": "array",
      "items": {
        "$ref": "#/data_file"
      },
      "import": {
        "ae": {
          "from_type": "array",
          "parent": "assay_data",
          "path": [
            "files"
          ],
          "method": "import_file"
        },
        "hca": {
          "path": [
            "sequence_file_json"
          ],
          "from_type": "array",
          "method": "import_nested_data_files"
        }
      }
    },
    "data_type": {
      "description": "raw or raw matrix",
      "type": "string",
      "import": {
        "ae": {
          "from_type": "string",
          "parent": "assay_data",
          "path": [
            "attributes",
            "data_type"
          ],
          "method": "get_string_from_attribute"
        },
        "hca": {
          "path": [],
          "method": "use_translation",
          "translation": "raw"
        }
      }
    },
    "assayrefs": {
      "description": "assay name/accessions",
      "type": "array",
      "import": {
        "ae": {
          "from_type": "array",
          "parent": "assay_data",
          "path": [
            "assayRefs"
          ],
          "method": "get_reference_value_from_json"
        },
        "hca": {
          "path": [],
          "method": "placeholder"
        }
      }
    },
    "accession": {
      "description": "(not for microarray) ENA run accession",
      "type": "string",
      "import": {
        "ae": {
          "from_type": "string",
          "parent": "assay_data",
          "path": [
            "accession"
          ],
          "method": "import_string"
        }
      }
    }
  },
    "data_file": {
    "name": {
      "type": "string",
      "import": {
        "ae": {
          "from_type": "string",
          "path": [
            "name"
          ],
          "method": "import_string"
        },
        "hca": {
          "path": [
            "file_core",
            "file_name"
          ],
          "from_type": "string",
          "method": "import_string_from_selected_entity"
        }
      }
    },
    "checksum": {
      "type": "string",
      "import": {
        "ae": {
          "from_type": "string",
          "path": [
            "checksum"
          ],
          "method": "import_string"
        },
        "hca": {
          "path": [
            "file_core",
            "checksum"
          ],
          "from_type": "string",
          "method": "import_string_from_selected_entity"
        }
      }
    },
    "checksum_method": {
      "type": "string",
      "import": {
        "ae": {
          "from_type": "string",
          "path": [
            "checksumMethod"
          ],
          "method": "import_string"
        },
        "hca": {
          "path": [],
          "from_type": "string",
          "method": "use_translation",
          "translation": "MD5"
        }
      }
    },
    "ftp_location": {
      "type": "string",
      "import": {
        "ae": {
          "from_type": "string",
          "path": [
            "attributes",
            "ftp_location"
          ],
          "method": "get_string_from_attribute"
        }
      }
    },
    "read": {
      "description": "The type of read for multiplexed single cell sequence files: read1/2 or index1/2",
      "type": "string",
      "import": {
        "ae": {
          "path": ["attributes", "read"]
        },
        "hca": {
          "path": [
            "read_index"
          ],
          "from_type": "string",
          "method": "import_string_from_selected_entity"
        }
      }
    }
  }
}