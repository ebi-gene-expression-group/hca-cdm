          "path": ["project_json", "project_core", "project_title"],
          "from_type": "sting",
          "method": "import_string"
        }
      }
    },
    "description": {
      "description": "free-text description of study background and aim",
      "type": "string",
      "import": {
        "ae": {
          "parent": "study",
          "path": [
            "description"
          ],
          "from_type": "string",
          "method": "import_string"
        },
        "hca": {
          "path": ["project_json", "project_core", "project_description"],
          "from_type": "sting",
          "method": "import_string"
        }
      }
    },
    "experimental_factor": {
      "description": "list of experimental factors as Attribute class objects",
      "type": "array",
      "items": {
        "$ref": "#definitions/attribute"
      },
      "import": {
        "ae": {
          "parent": "study",
          "path": [
            "attributes",
            "experimental_factor"
          ],
          "from_type": "attribute_object",
          "method": "generate_attribute_from_json"
        }
      }
    },
    "experimental_design": {
      "description": "(optional) list of experimental designs as Attribute class objects",
      "type": "array",
      "items": {
        "$ref": "#definitions/attribute"
      },
      "import": {
        "ae": {
          "parent": "study",
          "path": [
            "attributes",
            "experimental_design"
          ],
          "from_type": "attribute_object",
          "method": "generate_attribute_from_json"
        }
      }
    },
    "experiment_type": {
      "description": "experiment type ontology terms",
      "type": "array",
      "items": {
      },
      "import": {
        "ae": {
          "parent": "study",
          "path": [
            "attributes",
            "experiment_type"
          ],
          "from_type": "attribute_object",
          "method": "get_string_from_attribute"
        },
        "hca": {
          "path": [],
          "method": "get_experiment_type"
        }
      }
    },
    "date_of_experiment": {
      "description": "(optional) date of experiment",
      "type": "string",
      "import": {
        "ae": {
          "parent": "study",
          "path": [
            "attributes",
            "date_of_experiment"
          ],
          "from_type": "string",
          "method": "import_string"
        },
        "hca": {
          "path": [
            "project_json",
            "provenance",
            "submission_date"
          ],
          "from_type": "sting",
          "method": "import_string"
        }
      }
    },
    "protocolrefs": {
      "description": "protocol accessions/name used in the study",
      "type": "array",
      "import": {
        "ae": {
          "path": ["protocolRefs"],
          "from_type": "array",
          "method": "get_reference_value_from_json"
        },
        "hca": {
          "path" : [],
          "method": "placeholder"
        }
      }
    },
    "projectref": {
      "type": "string",
      "import": {
        "ae": {
          "path": ["projectRef"],
          "from_type": "ref_object",
          "method": "get_reference_value_from_json"
        },
        "hca": {
          "path" : [],
          "method": "placeholder"
        }
      }
    },
    "study_type": {
      "description": "submission type: microarray, sequencing, singlecell",
      "type": "string",
      "import": {
        "ae": {
          "parent": "study",
          "path": [
            "attributes",
            "study_type"
          ],
          "from_type": "attribute_object",
          "method": "get_string_from_attribute"
        },
        "hca": {
          "path" : [],
          "method": "get_study_type"
        }
      }
    }
  },
  "publication": {
    "articleTitle": {
