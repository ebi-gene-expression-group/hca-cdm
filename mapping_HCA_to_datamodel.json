{
  "project": {
    "alias": {
      "description": "unique project name (auto-generated from MAGE-TAB file name)",
      "type": "string",
      "import": {
        "ae": {
          "parent": "project",
          "path": [
            "alias"
          ],
          "from_type": "sting",
          "method": "import_string"
        },
        "hca": {
          "parent": "project_json",
          "path": [
            "project_core",
            "project_short_name"
          ],
          "from_type": "sting",
          "method": "import_string"
        }
      }
    },
    "accession": {
      "description": "BioStudies accession",
      "type": "string",
      "import": {
        "ae": {
          "parent": "project",
          "path": [
            "accession"
          ],
          "from_type": "sting",
          "method": "import_string"
        },
        "hca": {
          "parent": "project",
          "path": [
            "project",
            "biostudies_accessions"
          ],
          "from_type": "sting",
          "method": "import_string"
        }
      }
    },
    "title": {
      "description": "(optional) submitter provided project description",
      "type": "string",
      "import": {
        "ae": {
          "parent": "project",
          "path": [
            "title"
          ],
          "from_type": "sting",
          "method": "import_string"
        },
        "hca": {
          "parent": "project",
          "path": [
            "project",
            "project_core",
            "project_title"
          ],
          "from_type": "sting",
          "method": "import_string"
        }
      }
    },
    "description": {
      "description": "(optional) free-text project description",
      "type": "string",
      "import": {
        "ae": {
          "parent": "project",
          "path": [
            "description"
          ],
          "from_type": "sting",
          "method": "import_string"
        },
        "hca": {
          "parent": "project",
          "path": [
            "project",
            "project_core",
            "project_description"
          ],
          "from_type": "sting",
          "method": "import_string"
        }
      }
    },
    "releaseDate": {
      "description": "date of public release",
      "type": "string",
      "import": {
        "ae": {
          "parent": "project",
          "path": [
            "releaseDate"
          ],
          "from_type": "sting",
          "method": "import_string"
        }
      }
    },
    "publications": {
      "description": "list of Publication class objects",
      "type": "array",
      "items": {
        "$ref": "#/publication"
      },
      "import": {
        "ae": {
          "parent": "project",
          "path": [
            "publications"
          ],
          "from_type": "array",
          "method": "import_publication"
        },
        "hca": {
          "parent": "project",
          "path": [
            "project",
            "publications"
          ],
          "from_type": "array",
          "method": "import_publication"
        }
      },
      "contacts": {
        "description": "list of Contact class objects",
        "type": "array",
        "items": {
          "$ref": "#/definitions/contact"
        },
        "import": {
          "ae": {
            "parent": "project",
            "path": [
              "contacts"
            ],
            "from_type": "array",
            "method": "import_contact"
          },
          "hca": {
            "parent": "project",
            "path": [
              "project",
              "contributors"
            ],
            "from_type": "array",
            "method": "import_contact"
